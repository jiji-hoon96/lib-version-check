{
  "name": "lib-version-check",
  "version": "1.3.7",
  "description": "A command-line tool to easily track and check versions of your favorite NPM libraries",
  "type": "module",
  "bin": {
    "lib-check": "./bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "npm",
    "version",
    "check",
    "update",
    "library",
    "package",
    "version-checker",
    "npm-tool",
    "development-tool"
  ],
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check .",
    "biome:ci": "biome check --apply ."
  },
  "author": "jiji-hoon96",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiji-hoon96/lib-check"
  },
  "bugs": {
    "url": "https://github.com/jiji-hoon96/lib-check/issues"
  },
  "homepage": "https://github.com/jiji-hoon96/lib-check#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3"
  }
}
